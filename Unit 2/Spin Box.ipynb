   "source": [
    "from tkinter import *\n",
    "window = Tk()\n",
    "window.title('Scrolled Text')\n",
    "window.minsize(100,100)\n",
    "window.maxsize(800,800)\n",
    "window.geometry('300x200')\n",
    "window.iconbitmap(r'fav.ico')\n",
    "\n",
    "s = Spinbox(window,from_=0,to=10,width=20)\n",
    "s.pack()\n",
    "\n",
    "window.mainloop()"
   ]
